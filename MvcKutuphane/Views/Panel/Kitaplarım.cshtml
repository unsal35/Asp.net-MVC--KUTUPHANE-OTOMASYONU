@using MvcKutuphane.Models.Entity
@model List<TBLHareket>
@{
    ViewBag.Title = "Kitaplarım";
    Layout = "~/Views/Shared/_LayoutPanel.cshtml";
}

<h2>Kitap Geçmişim</h2>


<table class="table table-bordered">
    <tr>
       
        <th>Kitap</th>
        <th>Alış Tarihi</th>
        <th>İade Tarihi</th>
     
    </tr>
    @foreach (var item in Model)
    {
        <tr>
            <td>@item.TBLKitap.KitapAd</td>
            <td>@Convert.ToDateTime(@item.AlisTarihi).ToString("dd-MM-yyyy")</td>
            <td>@Convert.ToDateTime(@item.UyeGetirTarih).ToString("dd-MM-yyyy")</td>
        </tr>
    }
</table>

